    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Go with config",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/backend",
            "args": ["--config", "../local-secrets/config.json"]
        },
        {
            "name": "Research",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/research",
            "args": ["NL_MASTERLIST.mls"]
        },
        {
            "name": "SOD validator",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/research/sod_validator",
            "args": ["../samples/lds_content.der", "../samples/sample.json"]
        },
        {
            "name": "ML extractor",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/research/masterlist_extractor",
            "args": ["../samples/NL_MASTERLIST.mls"]
        }
    ]
}
